{
  "name": "darkflare",
  "version": "1.0.0-canary.0",
  "description": "The Cloudflare Framework",
  "types": "./types/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --format=cjs --platform=node --outfile=dist/index.js",
    "build:watch": "esbuild src/index.ts --bundle --minify --format=cjs --platform=node --watch --outfile=dist/index.js",
    "publish:canary": "npm run build && npm publish --tag canary",
    "publish:patch": "npm run build && npm version patch && npm publish",
    "publish:minor": "npm run build && npm version minor && npm publish",
    "publish:major": "npm run build && npm version major && npm publish",
    "test": "jest --detectOpenHandles"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/azury"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azurydev/darkflare.git"
  },
  "keywords": [
    "api",
    "workers",
    "cloudflare",
    "framework"
  ],
  "author": "Azury <hello@azury.dev> (https://azury.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/azurydev/darkflare/issues"
  },
  "homepage": "https://github.com/azurydev/darkflare#readme",
  "dependencies": {
    "esbuild": "^0.14.36"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.5.1",
    "@tsndr/cloudflare-worker-jwt": "^1.1.7",
    "@types/jest": "^27.4.1",
    "itty-router": "^2.6.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
